<?xml version="1.0" encoding="utf-8" ?>
<!-- used to test the subject line userData value 

test 1. test we get the default value if no value supplied
test 2. test we can load data into the subject line
-->
<emailTemplate application="TestApplication">
<name>Simple Template</name>
<description>Template used to test</description>
<mailFormat>PlainText</mailFormat>
<message>
  <to>emailtemplate@mailinator.com</to>
  <from>emailtemplate@mailinator.com</from>
	<subject>TestEmail <userData mandatory="false" default="Mark" name="SubjectData1"/></subject>
	<body>Sample email from email template processor
	<userData mandatory="false" default="Mark" name="UserData1"/>
	End Message
	</body>
	<attachments>
	</attachments>
</message>
</emailTemplate>
